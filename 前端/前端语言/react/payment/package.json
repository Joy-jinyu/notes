    "scripts": {
        "start": "cross-env DOMAIN=checkout-test.starservices.store node scripts/start.js",
        "build": "cross-env PUBLIC_URL=//static.starservices.store/checkout DOMAIN=checkout.starservices.store REPORT_DOMAIN=dot.cloudsdlk.com node scripts/build.js",
        "dev": "cross-env PUBLIC_URL=//onepageharvey.ngrok.starservices.store/checkout DOMAIN=onepageharvey.ngrok.starservices.store REPORT_DOMAIN=localhost node scripts/build.js",
        "dev2": "cross-env PUBLIC_URL=//onepagedev.ngrok.starservices.store/checkout cross-env DOMAIN=onepagedev.ngrok.starservices.store REPORT_DOMAIN=localhost node scripts/build.js",
        "test": "node scripts/test.js",
        "build-test": "PUBLIC_URL=//static-test.starservices.store/checkout DOMAIN=checkout-test.starservices.store REPORT_DOMAIN=localhost node scripts/build.js",
        "build-beta": "PUBLIC_URL=//checkout-beta.starservices.store/checkout DOMAIN=checkout-beta.starservices.store REPORT_DOMAIN=localhost node scripts/build.js",
        "build-temp": "PUBLIC_URL=//checkout-temp.starservices.store/checkout DOMAIN=checkout-temp.starservices.store REPORT_DOMAIN=localhost node scripts/build.js"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ],
        "rules": {
            "no-undef": "off",
            "no-restricted-globals": "off",
            "no-unused-vars": "off"
        }
    },
    "browserslist": {
        "production": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8",
            "Android >= 4.4",
            "iOS >= 10"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jsdom",
        "testRunner": "/Users/Raymond/node/test-act/node_modules/jest-circus/runner.js",
        "transform": {
            "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ],
        "resetMocks": true
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "react-test-renderer": "^17.0.2",
        "sass": "^1.45.0",
        "webpack-bundle-analyzer": "^3.8.0"
    },
    "resolutions": {
        "@babel/preset-env": "^7.8.0"
    }
}